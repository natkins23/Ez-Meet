{
  "name": "Ez-Meet Zoom",
  "description": "Connect automatically to your zoom meetings",
  "version": "2.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/EZlogo/ez-logo-3rd-16.png",
      "32": "/EZlogo/ez-logo-3rd-32.png",
      "48": "/EZlogo/ez-logo-3rd-48.png",
      "128": "/EZlogo/ezlogo128.png"
    }
  },
  "icons": {
    "16": "/EZlogo/ez-logo-3rd-16.png",
    "32": "/EZlogo/ez-logo-3rd-32.png",
    "48": "/EZlogo/ez-logo-3rd-48.png",
    "128": "/EZlogo/ezlogo128.png"
  }
}
